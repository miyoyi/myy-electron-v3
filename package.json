{
  "name": "vue3-vite-ts-electron-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "output/electron/main.cjs",
  "scripts": {
    "start": "vite --config ./config/vite.config.dev.ts",
    "build:win": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts && electron-builder -w",
    "build:mac": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts && electron-builder -m",
    "build:prev": "vue-tsc --noEmit && vite build --config ./config/vite.config.prod.ts",
    "preview": "electron output/electron/main.cjs",
    "lint-staged": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@arco-design/web-vue": "^2.50.0",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^2.4.0",
    "query-string": "^7.1.3",
    "sortablejs": "^1.15.0",
    "ts-md5": "^1.3.1",
    "vue": "^3.2.47",
    "vue-echarts": "^6.6.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/lodash": "^4.14.197",
    "@types/mockjs": "^1.0.7",
    "@types/nprogress": "^0.2.0",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/babel-plugin-jsx": "^1.1.5",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "electron": "^24.8.0",
    "electron-builder": "23.6.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^13.3.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "rollup": "^2.79.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "tsup": "^6.7.0",
